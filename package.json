    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
